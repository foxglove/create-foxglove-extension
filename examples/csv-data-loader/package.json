{
  "name": "csv-data-loader",
  "displayName": "CSV Foxglove Data Loader",
  "description": "",
  "publisher": "Foxglove",
  "version": "1.1.0",
  "license": "MIT",
  "main": "./dist/extension.js",
  "scripts": {
    "build": "npm run build:wasm && foxglove-extension build",
    "build:wasm": "cd rust && cargo build --release --target wasm32-unknown-unknown",
