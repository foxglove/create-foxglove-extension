    "prepack": "yarn build",
    "prepublishOnly": "yarn lint:ci && yarn test",
    "test": "jest",
    "watch": "tsc --watch"
  },
  "engines": {
    "node": ">= 14"
  },
  "devDependencies": {
    "@foxglove/eslint-plugin": "^0.21.0",
    "@types/jest": "^29.4.0",
    "@types/mkdirp": "^2.0.0",
    "@types/ncp": "^2",
    "@types/node": "^18.14.6",
    "@types/node-fetch": "^2.6.2",
    "@types/rimraf": "^4.0.5",
    "@types/tmp": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "css-loader": "6.7.3",
