    "clean-webpack-plugin": "4.0.0",
    "commander": "10.0.0",
    "jszip": "3.10.1",
    "mkdirp": "2.1.5",
    "ncp": "2.0.0",
    "node-fetch": "2.6.9",
    "path-browserify": "1.0.1",
    "rimraf": "4.3.1",
    "sanitize-filename": "1.6.3",
    "ts-loader": "9.5.1",
    "webpack": "5.92.1"
