    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "29.7.0",
    "prettier": "^2.8.4",
    "style-loader": "3.3.1",
    "tmp": "0.2.3",
    "typescript": "4.9.5",
    "yarn": "1.22.19"
